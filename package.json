{
	"name": "@daiyam/jsonc-preprocessor",
	"description": "Disable/enable/ignore blocks based on rules found in the JSONC text",
	"version": "0.2.0",
	"author": {
		"name": "Baptiste Augrain",
		"email": "daiyam@zokugun.org"
	},
	"license": "MIT",
	"homepage": "https://github.com/daiyam/node-jsonc-preprocessor",
	"repository": {
		"type": "git",
		"url": "https://github.com/daiyam/node-jsonc-preprocessor.git"
	},
	"bugs": {
		"url": "https://github.com/daiyam/node-jsonc-preprocessor/issues"
	},
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"commit": "cz",
		"compile": "tsc -p src",
		"lint": "xo",
		"prepare": "husky install; fixpack || true",
		"prepublishOnly": "npm run compile",
		"release": "release-it",
		"test": "tsc -p test && mocha",
		"test:dev": "mocha",
		"test:watch": "tsc-watch -p test --onSuccess 'mocha'",
		"watch": "tsc -watch -p src",
		"watch:test": "tsc-watch -p test"
	},
	"dependencies": {
		"@daiyam/balanced-pairs": "^0.1.1",
		"compare-versions": "^3.6.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"@types/chai": "^4.2.17",
		"@types/fs-extra": "^9.0.12",
		"@types/mocha": "^9.1.0",
		"@types/node": "^14.18.12",
		"chai": "^4.3.4",
		"commitizen": "^4.2.4",
		"eslint-plugin-chai-friendly": "^0.7.2",
		"fixpack": "^4.0.0",
		"fs-extra": "^10.0.0",
		"globby": "^11.1.0",
		"husky": "^7.0.1",
		"lint-staged": "^12.3.6",
		"mocha": "^9.2.2",
		"release-it": "^14.11.3",
		"source-map-support": "^0.5.20",
		"ts-node": "^10.1.0",
		"tsc-watch": "^4.4.0",
		"typescript": "^4.2.4",
		"xo": "^0.48.0",
		"yaml": "^1.10.2"
	},
	"keywords": [
		"attribute",
		"attributes",
		"decorator",
		"decorators",
		"jsonc",
		"preprocessor"
	]
}
